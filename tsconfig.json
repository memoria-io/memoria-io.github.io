{
  "compilerOptions":{
    "target":"esnext",
    "module":"esnext",
    "moduleResolution":"node",
    "strict":true,
    "jsx":"preserve",
    "esModuleInterop":true,
    "skipLibCheck":true,
    "forceConsistentCasingInFileNames":true,
    "baseUrl":".",
    "types":[
      "vite/client"
    ],
    "paths":{
      "@/*":[
        "src/*"
      ]
    }
  },
  "include":[
    "src"
  ],
  "exclude":[
    "node_modules"
  ]
}